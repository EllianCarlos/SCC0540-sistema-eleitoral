<html lang='pt-BR'>
  <head>
    <meta charset='utf-8' />
    <title>Sistema Eleitoral</title>
    <link rel='stylesheet' href='/style/global.css' />
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3'
      crossorigin='anonymous'
    />
  </head>

  <body>
    <script src="scripts/deleteEvent.js"></script>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>
    <header class='p-3 bg-dark text-white'>
      {{!-- <div class='container'> --}}
        <div class='d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start'>
          <a href='/' class='d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none'>
            <svg
              xmlns='http://www.w3.org/2000/svg'
              width='16'
              height='16'
              fill='currentColor'
              class='bi bi-easel2'
              viewBox='0 0 16 16'
            >
              <path
                fill-rule='evenodd'
                d='M8 0a.5.5 0 0 1 .447.276L8.81 1h4.69A1.5 1.5 0 0 1 15 2.5V11h.5a.5.5 0 0 1 0 1h-2.86l.845 3.379a.5.5 0 0 1-.97.242L12.11 14H3.89l-.405 1.621a.5.5 0 0 1-.97-.242L3.36 12H.5a.5.5 0 0 1 0-1H1V2.5A1.5 1.5 0 0 1 2.5 1h4.691l.362-.724A.5.5 0 0 1 8 0ZM2 11h12V2.5a.5.5 0 0 0-.5-.5h-11a.5.5 0 0 0-.5.5V11Zm9.61 1H4.39l-.25 1h7.72l-.25-1Z'
              ></path>
            </svg>
          </a>
          <ul class='nav col-6 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0'>
            <li><a href='/' class='nav-link px-3 text-secondary'>Home</a></li>
            <li><a href='cargos' class='nav-link px-3 text-white'>Cargos</a></li>
            <li><a href='pessoas' class='nav-link px-3 text-white'>Pessoa Fisica</a></li>
            <li><a href='pleitos' class='nav-link px-3 text-white'>Pleito</a></li>
            <li><a href='programas' class='nav-link px-3 text-white'>Programa</a></li>
            <li><a href='partidos' class='nav-link px-3 text-white'>Partido</a></li>
            <li><a href='processos' class='nav-link px-3 text-white'>Processo Judicial</a></li>
            <li><a href='candidaturas' class='nav-link px-3 text-white'>Candidatura</a></li>
            <li><a href='doacoesPf' class='nav-link px-3 text-white'>Doações PF</a></li>
            <li><a href='*' class='nav-link px-3 text-white'>Doações PJ</a></li>
            <li><a href='equipes' class='nav-link px-3 text-white'>Equipe</a></li>
            <li><a href='apoiadores' class='nav-link px-3 text-white'>Apoiador </a></li>
          </ul>
        </div>
      {{!-- </div> --}}
    </header>
    <div class='container'>
      <main class='table-responsive mt-3' role='main'>
        <table class="table table-striped table-sm">
            <thead>
              <tr>
                <th>CNPJ</th>
                <th>Nome</th>
                <th>Candidato</th>
                <th>Ano</th>
                <th>Data e Hora</th>
                <th>Valor</th>
                <th>Remover</th>
              </tr>
            </thead>
            {{#each doacoesPj}}
              <tr>
                <td>
                  {{this.cnpj_doador}}
                </td>
                <td>
                  {{this.nome}}
                </td>
                <td>
                  {{this.candidato}}
                </td>
                <td>
                  {{this.ano_candidatura}}
                </td>
                <td>
                  {{this.data_doacao}}
                </td>
                <td>
                  {{this.valor}}
                </td>
                <td>
                  <button type="button" class="btn btn-danger {{concat @index 'dclass'}}"onclick="{{delete ../table @index this.cnpj_doador this.candidato this.ano_candidatura}}">Remover</button>
                </td>
              </tr>
            {{/each}}
            </tbody>
        </table>
      </main>
    </div>
  </body>
</html>
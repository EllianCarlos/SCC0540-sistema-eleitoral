<html lang='pt-BR'>
  <head>
    <meta charset='utf-8' />
    <title>Sistema Eleitoral</title>
    <link rel='stylesheet' href='/style/global.css' />
    <link
      href='https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css'
      rel='stylesheet'
      integrity='sha384-1BmE4kWBq78iYhFldvKuhfTAU6auU8tT94WrHftjDbrCEXSU1oBoqyl2QvZ6jIW3'
      crossorigin='anonymous'
    />
  </head>

  <body>
    <script src='scripts/deleteEvent.js'></script>
    <script src='https://unpkg.com/axios/dist/axios.min.js'></script>
    <header class='p-3 bg-dark text-white'>
      {{! <div class='container'> }}
      <div class='d-flex flex-wrap align-items-center justify-content-center justify-content-lg-start'>
        <a href='/' class='d-flex align-items-center mb-2 mb-lg-0 text-white text-decoration-none'>
          <svg
            xmlns='http://www.w3.org/2000/svg'
            width='16'
            height='16'
            fill='currentColor'
            class='bi bi-easel2'
            viewBox='0 0 16 16'
          >
            <path
              fill-rule='evenodd'
              d='M8 0a.5.5 0 0 1 .447.276L8.81 1h4.69A1.5 1.5 0 0 1 15 2.5V11h.5a.5.5 0 0 1 0 1h-2.86l.845 3.379a.5.5 0 0 1-.97.242L12.11 14H3.89l-.405 1.621a.5.5 0 0 1-.97-.242L3.36 12H.5a.5.5 0 0 1 0-1H1V2.5A1.5 1.5 0 0 1 2.5 1h4.691l.362-.724A.5.5 0 0 1 8 0ZM2 11h12V2.5a.5.5 0 0 0-.5-.5h-11a.5.5 0 0 0-.5.5V11Zm9.61 1H4.39l-.25 1h7.72l-.25-1Z'
            ></path>
          </svg>
        </a>
        <ul class='nav col-6 col-lg-auto me-lg-auto mb-2 justify-content-center mb-md-0'>
          <li><a href='/' class='nav-link px-3 text-white'>Home</a></li>
          <li><a href='#' class='nav-link px-3 text-white'>Cargos</a></li>
          <li><a href='pessoas' class='nav-link px-3 text-white'>Pessoa Fisica</a></li>
          <li><a href='pleitos' class='nav-link px-3 text-white'>Pleito</a></li>
          <li><a href='programas' class='nav-link px-3 text-white'>Programa</a></li>
          <li><a href='partidos' class='nav-link px-3 text-white'>Partido</a></li>
          <li><a href='processos' class='nav-link px-3 text-white'>Processo Judicial</a></li>
          <li><a href='candidaturas' class='nav-link px-3 text-white'>Candidatura</a></li>
          <li><a href='doacoesPf' class='nav-link px-3 text-white'>Doações PF</a></li>
          <li><a href='doacoesPj' class='nav-link px-3 text-white'>Doações PJ</a></li>
          <li><a href='equipes' class='nav-link px-3 text-white'>Equipe</a></li>
          <li><a href='apoiadores' class='nav-link px-3 text-white'>Apoiador </a></li>
          <li><a href='ficha_limpa' class='nav-link px-3 text-white'>Ficha Limpa</a></li>
        </ul>
      </div>
      {{! </div> }}
    </header>
    <div class='container'>
      <main class='mt-3' role='main'>
        <p class='h1'>
          Dados da Candidatura
        </p>
        <b>Cargo:</b>
        {{candidaturas.0.cargo}}
        <br />
        <b>Abragência:</b>
        {{candidaturas.0.abrangencia}}<br />
        {{#if candidaturas.0.municipio}}<b>Municipio:</b> {{candidaturas.0.municipio}} <br /> {{/if}}
        {{#if candidaturas.0.estado}}<b>Estado:</b> {{candidaturas.0.estado}} <br /> {{/if}}
        <b>País:</b>
        {{candidaturas.0.pais}}
        <p class='small'>
          Candidatos ordenados por número de votos.
        </p>
        {{#each candidaturas}}
          <div class='container mt-4'>
            <h2>Pleito: {{pleito}}</h2>
            <b>Data do Pleito:</b>
            {{better_date data_do_pleito}}
            <br />
            <b>Número de Votos:</b>
            {{num_votos}}
          </div>
          <div class='container mt-4'>
            <div class='d-flex justify-content-around w-100'>
              <div class='w-25'>
                <div class='card' style='width: 18rem;'>
                  <div class='card-body'>
                    <h5 class='card-title'>{{nome}}</h5>
                    <small class='card-title'>Candidato</small>
                  </div>
                  <ul class='list-group list-group-flush'>
                    <li class='list-group-item'>Data de Nascimento: {{better_date data_nasc}} </li>
                    <li class='list-group-item'>Função Atual: {{funcao_atual}} </li>
                    <li class='list-group-item'>CPF: {{candidato}} </li>
                    <li class='list-group-item'>Partido: {{partido_do_candidato}} </li>
                    <li class='list-group-item'>Sigla do Partido: {{partido_do_candidato_sigla}} </li>
                    <li class='list-group-item'>Programa do Partido: {{partido_do_candidato_programa}} </li>
                    <li class='list-group-item'>Presidente do Partido: {{partido_do_candidato_presidente}} </li>
                  </ul>
                </div>
              </div>
              {{#if vice_nome}}
                <div class='w-25'>
                  <div class='card' style='width: 18rem;'>
                    <div class='card-body'>
                      <h5 class='card-title'>{{vice_nome}}</h5>
                      <small class='card-title'>Vice Candidato</small>
                    </div>
                    <ul class='list-group list-group-flush'>
                      <li class='list-group-item'>Data de Nascimento: {{better_date vice_data_nasc}} </li>
                      <li class='list-group-item'>Função Atual: {{vice_funcao}} </li>
                      <li class='list-group-item'>CPF: {{vice_cpf}} </li>
                      <li class='list-group-item'>Partido: {{partido_do_candidato}} </li>
                      <li class='list-group-item'>Sigla do Partido: {{partido_do_vice_sigla}} </li>
                      <li class='list-group-item'>Programa do Partido: {{partido_do_vice_programa}} </li>
                      <li class='list-group-item'>Presidente do Partido: {{partido_do_vice_presidente}} </li>
                    </ul>
                  </div>
                </div>
              {{/if}}
            </div>
          </div>
        {{/each}}
      </main>
    </div>
  </body>
</html>